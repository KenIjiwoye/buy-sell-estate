<p id="notice"><%= notice %></p>

<h1>Properties</h1>

<table>
  <thead>
    <tr>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Description</th>
      <th>Price</th>
      <th>Propertytype</th>
      <th>Status</th>
      <th>Area</th>
      <th>Beds</th>
      <th>Baths</th>
      <th>Garage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @properties.each do |property| %>
      <tr>
        <td><%= property.address1 %></td>
        <td><%= property.address2 %></td>
        <td><%= property.city %></td>
        <td><%= property.state %></td>
        <td><%= property.zipcode %></td>
        <td><%= property.description %></td>
        <td><%= property.price %></td>
        <td><%= property.propertyType %></td>
        <td><%= property.status %></td>
        <td><%= property.area %></td>
        <td><%= property.beds %></td>
        <td><%= property.baths %></td>
        <td><%= property.garage %></td>
        <td><%= link_to 'Show', property %></td>
        <td><%= link_to 'Edit', edit_property_path(property) %></td>
        <td><%= link_to 'Destroy', property, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Property', new_property_path %>
